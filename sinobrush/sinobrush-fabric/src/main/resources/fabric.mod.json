{
    "schemaVersion": 1,
    "id": "${modid}",
    "version": "${sinoseries_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${sinoseries_authors}"
    ],
    "contact": {
        "homepage": "${sinoseries_url}",
        "sources": "${sinoseries_source}",
        "issues": "${sinoseries_issues}"
    },
    "license": "${sinoseries_license}",
    "icon": "sinobrush_logo.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "games.moegirl.sinocraft.sinobrush.fabric.SinoBrushFabric"
        ],
        "client": [
            "games.moegirl.sinocraft.sinobrush.fabric.SinoBrushFabric"
        ]
    },
    "mixins": [
        "sinobrush-common.mixins.json"
    ],
    "depends": {
        "java": ">=17",
        "minecraft": "~${minecraft_version}",
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "sinocore": ">=${sinoseries_version}"
    }
}